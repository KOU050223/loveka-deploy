{
    "builds": [
      {
        "src": "index.mjs",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.mjs"
      }
    ],
    "files": [
      "views/**/*.ejs",
      "index.mjs",
      "package.json",
      "node_modules/**"
    ]    
  }
  